<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="my.test.IAnything">
    <method name="search">
      <arg type="as" direction="out"/>
      <arg name="path" type="s" direction="in"/>
      <arg name="keywords" type="s" direction="in"/>
      <arg name="offset" type="i" direction="in"/>
      <arg name="max_count" type="i" direction="in"/>
    </method>
    <method name="removePath">
      <arg type="b" direction="out"/>
      <arg name="fullPath" type="s" direction="in"/>
    </method>
    <method name="hasLFT">
      <arg type="b" direction="out"/>
      <arg name="path" type="s" direction="in"/>
    </method>
    <method name="addPath">
      <arg name="fullPath" type="s" direction="in"/>
    </method>
    <method name="index_files_in_directory">
      <arg name="directory_path" type="s" direction="in"/>
    </method>
  </interface>
</node>
